	nix astro-language-server
	config {
		typescript {
			tsdk "node_modules/typescript/lib"
		}
	}
}
// Lua
lua-language-server {
	source "https://github.com/LuaLS/lua-language-server"
	nix lua-language-server
	config {
		Lua {
			hint {
				enable #true
				arrayIndex Enable
				setType #true
				paramName All
				paramType #true
				await #true
			}
		}
	}
}
// Go
gopls {
	source "https://github.com/golang/tools/tree/master/gopls"
	nix gopls
	config {
		hints {
			assignVariableTypes #true
			compositeLiteralFields #true
			constantValues #true
			functionTypeParameters #true
			parameterNames #true
			rangeVariableTypes #true
		}
	}
}
golangci-lint-lsp golangci-lint-langserver {
	source "https://github.com/nametake/golangci-lint-langserver"
	nix golangci-lint-langserver
	config {
		command golangci-lint run "--output.json.path=stdout" "--show-stats=false" "--issues-exit-code=1"
	}
}
// Rust
rust-analyzer {
	source "https://github.com/rust-lang/rust-analyzer"
	nix rust-analyzer
	config {
		inlayHints {
			bindingModeHints {
				enable #false
			}
			closingBraceHints {
				minLines 10
			}
			closureReturnTypeHints {
				enable with_block
			}
			discriminantHints {
				enable fieldless
			}
			lifetimeElisionHints {
				enable skip_trivial
			}
			typeHints {
				hideClosureInitialization #false
			}
		}
		files {
			watcher server
		}
	}
}
// TypeScript/JavaScript
typescript-language-server {
	args --stdio
	source "https://github.com/typescript-language-server/typescript-language-server"
	nix typescript-language-server
	config {
		hostInfo helix
		typescript {
			inlayHints {
				includeInlayEnumMemberValueHints #true
				includeInlayFunctionLikeReturnTypeHints #true
				includeInlayFunctionParameterTypeHints #true
				includeInlayParameterNameHints all
				includeInlayParameterNameHintsWhenArgumentMatchesName #true
				includeInlayPropertyDeclarationTypeHints #true
				includeInlayVariableTypeHints #true
			}
		}
		javascript {
			inlayHints {
				includeInlayEnumMemberValueHints #true
				includeInlayFunctionLikeReturnTypeHints #true
				includeInlayFunctionParameterTypeHints #true
				includeInlayParameterNameHints all
				includeInlayParameterNameHintsWhenArgumentMatchesName #true
				includeInlayPropertyDeclarationTypeHints #true
				includeInlayVariableTypeHints #true
			}
		}
	}
}
// Svelte
svelteserver {
	args --stdio
	source "https://github.com/sveltejs/language-tools"
	nix svelte-language-server
	config {
		configuration {
			typescript {
				inlayHints {
					parameterTypes {
						enabled #true
					}
					variableTypes {
						enabled #true
					}
					propertyDeclarationTypes {
						enabled #true
					}
					functionLikeReturnTypes {
						enabled #true
					}
					enumMemberValues {
						enabled #true
					}
					parameterNames {
						enabled all
					}
				}
			}
			javascript {
				inlayHints {
					parameterTypes {
						enabled #true
					}
					variableTypes {
						enabled #true
					}
					propertyDeclarationTypes {
						enabled #true
					}
					functionLikeReturnTypes {
						enabled #true
					}
					enumMemberValues {
						enabled #true
					}
					parameterNames {
						enabled all
					}
				}
			}
		}
	}
}
// ESLint
vscode-eslint-language-server {
	args --stdio
	source "https://github.com/microsoft/vscode-eslint"
	nix vscode-langservers-extracted
	config {
		validate on
		run onType
		nodePath ""
		experimental {
			useFlatConfig #false
		}
		problems {
			shortenToSingleLine #false
		}
		codeAction {
			disableRuleComment {
				enable #true
				location separateLine
			}
			showDocumentation {
				enable #true
			}
		}
		workingDirectory {
			mode location
		}
	}
}
// Clarity
clarinet {
	args lsp
	source "https://github.com/hirosystems/clarinet"
	nix #false
}
// KCL
kcl-lsp kcl-language-server {
	args server --stdio
	source "https://github.com/kcl-lang/kcl"
	nix kcl
}
