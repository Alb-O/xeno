// Language configuration.
// LSP definitions are in lsp.kdl.
// Formatter definitions are in formatters.kdl.
use-grammars {
	except wren gemini
}
language name=rust scope=source.rust injection-regex=rs|rust auto-format=#true {
	file-types rs
	roots Cargo.toml Cargo.lock
	shebangs rust-script cargo
	comment-tokens "//" "///" "//!"
	block-comment-tokens {
		- start="/*" end="*/"
		- start="/**" end="*/"
		- start="/*!" end="*/"
	}
	language-servers rust-analyzer
	indent tab-width=4 unit="    "
	persistent-diagnostic-sources rustc clippy
	auto-pairs "("=")" "{"="}" "["="]" "\""="\"" `=`
	debugger lldb-dap-rust
}
language name=sway scope=source.sway injection-regex=sway comment-token="//" {
